#For printing prime  umers from 1 to 100
i = 2

while i <= 100:
    j = 2
    while j < 100:
        if i%j == 0:
            break
        j += 1
    if i == j:
        print(i)
    i += 1


#Factorial of a number which is given by the user
value=int(input("Enter a number"))
fact=1
i=1
while value>=1:
    fact=fact*value
    value=value
print(fact)


#Printing a table of a number given by user
value=int(input("Enter a number"))
i=1
while i<=10:
    print(str(value)+"X"+str(i)+"="+str(value*i))
    i=i+1



#printing even number from starting till the number given by the user.
value=int(input("Enter a number"))
i=1
while i<=value:
    if (i%2==0):
        print(i)
    i=i+1



#count the nunber of vowel in the given string
value=input("Enter a sring")
i=0
count=0
while i<len(value):
    if value[i] in("a","e","i","o","u"):
        count=count+1
    i=i+1
print(count)  



#for calculating and printing the sum of the square .
value=int(input("Enter a number"))

i=0
total=0

while i<=value:
    total=total+i*i
    i=i+1

print(total) 

